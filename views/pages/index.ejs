<!DOCTYPE>
<html>
<head>
    <link rel="stylesheet" type="text/css" href="/stylesheets/main.css">
</head>
<body>
    <h1>developer's blog</h1>
    <% blogPostJson.forEach(function(row) { %>
    <div class="blog-post"></div>
    <h1><%= row.title %></h1>
    <p><%= row.content %></p>
    <p>by: <%= row.admin_id %></p>

    <% }); %>
</body>
</html>